@page
@model ScoreManagement.Pages.AdminMenu.StudentsCoursesManage.DetailsModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>Details</h1>

<div>
    <h4>StudentsCourse</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.StudentsCourse.Class)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.StudentsCourse.Class.ClassCode)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.StudentsCourse.Student)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.StudentsCourse.Student.StudentCode)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.StudentsCourse.Course)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.StudentsCourse.Course.CourseName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.StudentsCourse.Semester)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.StudentsCourse.Semester.SemesterCode)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.StudentsCourse.Lecturer)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.StudentsCourse.Lecturer.LecturerName)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.StudentsCourse?.StudentCourseId">Edit</a> |
    <a asp-page="./Index">Back to List</a>
</div>
