@page
@model ScoreManagement.Pages.AdminMenu.SemestersManage.DetailsModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>Details</h1>

<div>
    <h4>Semester</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Semester.SemesterCode)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Semester.SemesterCode)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Semester.StartDate)
        </dt>
        <dd class="col-sm-10">
            @Model.Semester.StartDate?.ToString("dd/MM/yyyy")
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Semester.EndDate)
        </dt>
        <dd class="col-sm-10">
            @Model.Semester.EndDate?.ToString("dd/MM/yyyy")
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.Semester?.SemesterId">Edit</a> |
    <a asp-page="./Index">Back to List</a>
</div>
